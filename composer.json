{
    "name": "laravel-at/laravel-image-sanitize",
    "description": ":package_description",
    "keywords": [
        "image",
        "laravel-image-sanitize"
    ],
    "homepage": "https://github.com/laravel-at/laravel-image-sanitize",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian NÃ¼rnberger",
            "email": "adrian@nuernberger.me",
            "homepage": "https://nuernberger.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "illuminate/http": "^5.8",
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "LaravelAt\\ImageSanitize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAt\\ImageSanitize\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAt\\ImageSanitize\\ImageSanitizeServiceProvider"
            ],
            "aliases": {
                "ImageSanitize": "LaravelAt\\ImageSanitize\\ImageSanitizeFacade"
            }
        }
    }
}
